default: &default
  adapter: mysql2
  encoding: utf8mb4
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>
  username: <%= Rails.application.credentials.dig(:mysql, :username, :default) %>
  password: <%= Rails.application.credentials.dig(:mysql, :password, :default) %>
  socket: /var/run/mysqld/mysqld.sock

development:
  <<: *default
  database: CONSEN

production:
  <<: *default
  database: CONSEN



